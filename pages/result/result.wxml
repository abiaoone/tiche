<view class="container">
   <view class="ha">
          <label>{{maxIndex}} {{sex}}</label>
   </view>
    <view class="header">
             <p class="header_title">您的体质为：</p>
            
    </view>
     <view class="result">
             <p class="header_result">{{result}}</p>   
    </view>

    <view>
        <button id="btn" type="primary" size="{{default}}"  loading="{{load}}" bindtap="gotoDetail">查看说明</button>
        <button id="btn" type="primary" size="{{default}}"  loading="{{load}}" bindtap="testagige">再测一次</button>
        <button id="btn" type="primary" size="{{default}}"  loading="{{load}}" open-type="share">分享朋友圈</button>
        <button id="btn" type="default" size="{{default}}"  loading="{{load}}" bindtap="goBack">返回首页</button>
    </view>

</view>
